{
  "name": "<%= _.slugify(websiteName) %>",
  "version": "0.0.1",
  "dependencies": {<% if (includeCreate) { %>
    "createjs-preloadjs": "*",
    "createjs-tweenjs": "*",
    "createjs-soundjs": "*",
    "easeljs": "*",<% } if (includePolyfill) { %>
    "css3pie": "*",
    "selectivizr": "*",
    "background-size-polyfill": "*",
    "box-sizing-polyfill": "*",
    "respond": "*",
    "css3-mediaqueries-js": "*",<% } %>
    "jquery": "*",<% if (includeJqueryUi) { %>
    "jqueryui": "*",
    "jqueryui-touch-punch": "*",<% } if (includeJqueryPlugins) { %>
    "jquery-backstretch": "*",
    "Buttons": "*",<% } if (includeExample) { %>
    "FitText.js": "https://github.com/Milanowicz/FitText.js.git",
    "BrowserDetection.js": "https://github.com/Milanowicz/BrowserDetection.js.git",<% } if (includeMasonry) { %>
    "masonry": "*",
    "imagesloaded": "*",<% } if (includeModernizr) { %>
    "modernizr": "*",<% } %>
    "font-awesome": "~4.0.3",
    "CSSLessCollection": "https://github.com/Milanowicz/CSSLessCollection.git",
    "normalize-css": "*"
  }
}

