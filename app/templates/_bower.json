{
  "name": "<%= _.slugify(websiteName) %>",
  "version": "0.0.0",
  "dependencies": {<% if (includeCreate) { %>
    "createjs-preloadjs": "*",
    "createjs-tweenjs": "*",
    "createjs-soundjs": "*",
    "easeljs": "*",<% } if (includePolyfill) { %>
    "css3pie": "*",
    "selectivizr": "~1.0",
    "background-size-polyfill": "~0.2",
    "box-sizing-polyfill": "*",
    "respond": "~1.4",
    "css3-mediaqueries-js": "*",<% } if (jQueryVerion == 'One') { %>
    "jquery": "~1.11",<% } if (jQueryVerion == 'Two') { %>
    "jquery": "~2.1",<% } if (includeJqueryUi) { %>
    "jqueryui": "~1.10",
    "jqueryui-touch-punch": "*",<% } if (includeJqueryPlugins) { %>
    "jquery-backstretch": "*",
    "jquery-hashchange": "~",<% } if (includeButtons) { %>
    "Buttons": "*",<% } if (includeFitText) { %>
    "FitText.js": "git://github.com/Milanowicz/FitText.js.git",<% } if (includeBrowserDetection) { %>
    "BrowserDetection.js": "git://github.com/Milanowicz/BrowserDetection.js.git",<% } if (includeMasonry) { %>
    "masonry": "~3.1",
    "imagesloaded": "~3.1",<% } if (includeModernizr) { %>
    "modernizr": "~2.8",<% } if (includeFontAwesome) { %>
    "font-awesome": "~4.0",<% } if (supportLess) { %>
    "CSSLessCollection": "git://github.com/Milanowicz/CSSLessCollection.git",<% } if (supportSass) { %>
    "CSSSassCollection": "git://github.com/Milanowicz/CSSSassCollection.git",<% } %>
    "normalize-css": "~3.0"
  }
}

